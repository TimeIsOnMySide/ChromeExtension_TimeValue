{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "TimeValue",
    "author": "Carly Burkhart",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "content_scripts":
    [
        {
            "matches": ["*://*/*"],
            "all_frames": true,
            "js": ["content_script.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "storage"
      ]
}